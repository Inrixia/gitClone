		"@types/args": "^5.0.0",
		"args": "^5.0.3",
		"@octokit/core": "^4.1.0"
	},
	"devDependencies": {
		"@types/node": "^18.11.8",
		"pkg": "^5.8.0",
		"typescript": "^4.8.4"
	}
